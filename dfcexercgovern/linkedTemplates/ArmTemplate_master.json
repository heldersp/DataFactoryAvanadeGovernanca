{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfcexercgovern"
		},
		"AzureDataLakeStorageexercgovern_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageexercgovern'"
		},
		"AzureMySqlexercgovernSakila_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureMySqlexercgovernSakila'"
		},
		"MySqlexercgovern_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MySqlexercgovern'"
		},
		"AzureDataLakeStorageexercgovern_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storaccexercgovern.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfcexercgovern_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageexercgovern_accountKey": {
						"value": "[parameters('AzureDataLakeStorageexercgovern_accountKey')]"
					},
					"AzureMySqlexercgovernSakila_connectionString": {
						"value": "[parameters('AzureMySqlexercgovernSakila_connectionString')]"
					},
					"MySqlexercgovern_connectionString": {
						"value": "[parameters('MySqlexercgovern_connectionString')]"
					},
					"AzureDataLakeStorageexercgovern_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageexercgovern_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}